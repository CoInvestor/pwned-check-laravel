{
    "name": "square1/pwned-check-laravel",
    "description": "Validate password changes against known-compromised passwords",
    "type": "library",
    "keywords": [
        "square1",
        "pwned-check",
        "password",
        "security",
        "laravel"
    ],
    "homepage": "https://github.com/square1-io/pwned-check-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Conroy",
            "email": "paul@square1.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Square1\\Laravel\\PwnedCheck\\": "src"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "square1/pwned-check": "dev-master#d7c5258669600aef817f0887ea1ba31d8529d3f1"
    },
     "repositories": [
        {
            "type": "vcs",
            "name": "square1/pwned-check",
            "url": "https://github.com/Xulai/pwned-check.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Square1\\Laravel\\PwnedCheck\\Providers\\PwnedCheckServiceProvider"
            ]
        }
    }
}
